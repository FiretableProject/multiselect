{
  "version": "0.1.11",
  "license": "MIT",
  "name": "@rowy/multiselect",
  "publishConfig": {
    "access": "public"
  },
  "author": "Sidney Alcantara",
  "module": "dist/multiselect.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build; build-storybook -c .storybook -o docs; git add docs",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "react": "^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "size-limit": [
    {
      "path": "dist/test.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "@storybook/addon-actions": "6.3.8",
    "@storybook/addon-docs": "6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.3.8",
    "@storybook/addon-storysource": "6.3.8",
    "@storybook/addons": "6.3.8",
    "@storybook/react": "6.3.8",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "babel-loader": "^8.2.2",
    "dompurify": "2.3.0",
    "husky": "^4",
    "react": "^17.0.2",
    "react-div-100vh": "^0.6.0",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "tsdx": "0.14.1",
    "tslib": "2.3.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {}
}
